#include <stdio.h>
void main() 
{
    int duty, period;
    printf("Enter duty%% and period (PWM3): ");
    scanf("%d %d", &duty, &period);
    int on_time = (duty * period) / 100;
    printf("PWM 3: ");
    for (int i = 0; i < period; i++) 
{
        printf(i < on_time ? "1" : "0");
}
    printf("\n");
}
