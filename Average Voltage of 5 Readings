#include <stdio.h>
float averageVoltage(float v[], int n) 
{
    float sum = 0;
    for(int i = 0; i < n; i++)
        sum += v[i];
    return sum / n;
}
int main() 
{
    float v[5];
    printf("Enter 5 voltage readings: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &v[i]);
    printf("Average Voltage = %.2f V\n", averageVoltage(v, 5));
    return 0;
}
