#include <stdio.h>
float minValue(float arr[], int n) 
{
    float min = arr[0];
    for(int i = 1; i < n; i++)
        if(arr[i] < min)
            min = arr[i];
    return min;
}
int main() 
{
    float nums[5];
    printf("Enter 5 numbers: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &nums[i]);
    printf("Minimum = %.2f\n", minValue(nums, 5));
    return 0;
}
