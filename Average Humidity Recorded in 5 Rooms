#include <stdio.h>
float avgHumidity(float h[], int n) 
{
    float sum = 0;
    for(int i = 0; i < n; i++)
        sum += h[i];
    return sum / n;
}
int main() 
{
    float h[5];
    printf("Enter humidity in 5 rooms (in %%): ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &h[i]);
    printf("Average Humidity = %.2f%%\n", avgHumidity(h, 5));
    return 0;
}
