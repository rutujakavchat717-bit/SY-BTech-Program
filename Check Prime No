#include<stdio.h>

void main() 
{
    int n, isPrime = 1;
    printf("Enter number: ");
    scanf("%d", &n);

    for(int i = 2; i <= n/2; i++) {
        if(n % i == 0) {
            isPrime = 0;
            break;
        }
    }

    if(n < 2)
        printf("Not Prime");
    else if(isPrime)
        printf("Prime");
    else
        printf("Not Prime");
}
