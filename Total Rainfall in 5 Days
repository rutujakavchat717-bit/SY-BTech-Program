#include <stdio.h>
float totalRain(float rain[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += rain[i];
    return total;
}
int main() 
{
    float rain[5];
    printf("Enter rainfall (in mm) for 5 days: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &rain[i]);
    printf("Total Rainfall = %.2f mm\n", totalRain(rain, 5));
    return 0;
}
