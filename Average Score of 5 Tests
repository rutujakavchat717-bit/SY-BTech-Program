#include <stdio.h>
float avgScore(float s[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += s[i];
    return total / n;
}
int main() 
{
    float s[5];
    printf("Enter scores of 5 tests: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &s[i]);
    printf("Average Test Score = %.2f\n", avgScore(s, 5));
    return 0;
}
