#include <stdio.h>
#include <unistd.h>
void main()
{
int period = 10;
    printf("PWM HARD 9: Duty Sweep\n");
    for (int duty = 0; duty <= 10; duty++) 
{
        printf("%2d%%: ", duty * 10);
        for (int i = 0; i < period; i++)
            printf(i < duty ? "1" : "0");
        printf("\n");
 }
}
