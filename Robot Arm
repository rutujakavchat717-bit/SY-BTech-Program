#include <stdio.h>
#include <string.h>
int main() 
{
    char action[10];
    printf("Enter Action (PICK / PLACE): ");
    scanf("%s", action);
    if (strcmp(action, "PICK") == 0)
        printf("Robot Arm Picking Object\n");
    else if (strcmp(action, "PLACE") == 0)
        printf("Robot Arm Placing Object\n");
    else
        printf("Invalid Action\n");
    return 0;
}
