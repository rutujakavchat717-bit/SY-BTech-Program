#include <stdio.h>
float avgAge(float a[], int n) 
{
    float sum = 0;
    for(int i = 0; i < n; i++)
        sum += a[i];
    return sum / n;
}
int main() 
{
    float a[5];
    printf("Enter ages of 5 employees: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &a[i]);
    printf("Average Age = %.2f years\n", avgAge(a, 5));
    return 0;
}
