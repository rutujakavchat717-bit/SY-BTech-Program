#include <stdio.h>
#include <string.h>
int main() 
{
    char action[10];
    printf("Enter Command (SET / CANCEL): ");
    scanf("%s", action);
    if (strcmp(action, "SET") == 0)
        printf("Alarm Set Successfully\n");
    else if (strcmp(action, "CANCEL") == 0)
        printf("Alarm Cancelled\n");
    else
        printf("Invalid Command\n");
    return 0;
}
