#include <stdio.h>

struct Printer 
{
    int id;
    char model[30];
    float inkLevel;
    char status[20];
};

int main() 
{
    struct Printer p;

    printf("Enter Printer ID: ");
    scanf("%d", &p.id);

    printf("Enter Model: ");
    scanf("%s", p.model);

    printf("Enter Ink Level: ");
    scanf("%f", &p.inkLevel);

    printf("Enter Status: ");
    scanf("%s", p.status);

    printf("\n PRINTER INFORMATION \n");
    printf("ID: %d\n", p.id);
    printf("Model: %s\n", p.model);
    printf("Ink Level: %.2f\n", p.inkLevel);
    printf("Status: %s\n", p.status);

    return 0;
}
