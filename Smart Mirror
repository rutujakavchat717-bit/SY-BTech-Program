#include <stdio.h>
#include <string.h>
int main() 
{
    char state[10];
    printf("Enter Command (ON / OFF): ");
    scanf("%s", state);
    if (strcmp(state, "ON") == 0)
        printf("Smart Mirror Display ON\n");
    else if (strcmp(state, "OFF") == 0)
        printf("Smart Mirror Display OFF\n");
    else
        printf("Invalid Command\n");
    return 0;
}
