#include <stdio.h>
#include <string.h>
int main() 
{
    char mode[10];
    printf("Enter Mode (ON / OFF): ");
    scanf("%s", mode);
    if (strcmp(mode, "ON") == 0)
        printf("Heater Turned ON\n");
    else if (strcmp(mode, "OFF") == 0)
        printf("Heater Turned OFF\n");
    else
        printf("Invalid Mode\n");
    return 0;
}
