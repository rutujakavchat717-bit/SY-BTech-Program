#include<stdio.h>

void main() 
{
    int n, rev = 0, temp, rem;
    printf("Enter number: ");
    scanf("%d", &n);
    temp = n;

    while(n != 0) {
        rem = n % 10;
        rev = rev * 10 + rem;
        n /= 10;
    }

    if(rev == temp)
        printf("Palindrome");
    else
        printf("Not Palindrome");
}
