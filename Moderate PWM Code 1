#include <stdio.h>
void main()
{
    int duty_percent, period;
    printf("Enter duty cycle in %%: ");
    scanf("%d", &duty_percent);
    printf("Enter period: ");
    scanf("%d", &period);

    int duty_cycle = (duty_percent * period) / 100;
    printf("PWM 1: ");
    for(int i = 0; i < period; i++)
        printf(i < duty_cycle ? "1" : "0");
    printf("\n");
}
