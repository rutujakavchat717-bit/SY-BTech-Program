#include <stdio.h>
float totalFlow(float f[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += f[i];
    return total;
}
int main() 
{
    float f[5];
    printf("Enter water flow readings (in liters/hour): ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &f[i]);
    printf("Total Water Flow = %.2f liters\n", totalFlow(f, 5));
    return 0;
}
