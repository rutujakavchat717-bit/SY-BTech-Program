#include <stdio.h>
float totalBooks(float b[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += b[i];
    return total;
}
int main() 
{
    float b[5];
    printf("Enter number of books read each month: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &b[i]);
    printf("Total Books Read = %.0f\n", totalBooks(b, 5));
    return 0;
}
