#include <stdio.h>

void main() 
{
    float weight[5] = {5.5, 6.8, 7.4, 8.2, 9.0};
    float *ptr;
    int i;

    ptr = weight;

    printf("Weight Values:\n");
    for(i=0; i<5; i++) 
    {
        printf("weight[%d] = %.2f\n", i, weight[i]);
    }

    printf("\nDisplaying using pointer:\n");
    for(i=0; i<5; i++) 
    {
        printf("Weight %d: %.2f\n", i+1, *(ptr+i));
    }
}
