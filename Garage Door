#include <stdio.h>
#include <string.h>
int main() 
{
    char action[10];
    printf("Enter Command (OPEN / CLOSE): ");
    scanf("%s", action);
    if (strcmp(action, "OPEN") == 0)
        printf("Garage Door Opening\n");
    else if (strcmp(action, "CLOSE") == 0)
        printf("Garage Door Closing\n");
    else
        printf("Invalid Command\n");
    return 0;
}
