#include <stdio.h>
#include <string.h>
int main() 
{
    char command[10];
    printf("Enter Command (MOW / STOP): ");
    scanf("%s", command);
    if (strcmp(command, "MOW") == 0)
        printf("Lawn Mower Started Cutting Grass\n");
    else if (strcmp(command, "STOP") == 0)
        printf("Lawn Mower Stopped\n");
    else
        printf("Invalid Command\n");
    return 0;
}
