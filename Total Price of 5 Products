#include <stdio.h>
float totalPrice(float p[], int n) 
{
    float sum = 0;
    for(int i = 0; i < n; i++)
        sum += p[i];
    return sum;
}
int main() 
{
    float p[5];
    printf("Enter price of 5 products: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &p[i]);
    printf("Total Price = %.2f\n", totalPrice(p, 5));
    return 0;
}
