#include <stdio.h>

void main() 
{
    float signal[5] = {-50.5, -45.8, -40.1, -35.3, -30.2};
    float *ptr;
    int i;

    ptr = signal;

    printf("RF Signal Strength (dBm):\n");
    for(i=0; i<5; i++) 
    {
        printf("signal[%d] = %.2f\n", i, signal[i]);
    }

    printf("\nPointer Output:\n");
    for(i=0; i<5; i++) 
    {
        printf("Signal %d: %.2f dBm\n", i+1, *(ptr+i));
    }
}
