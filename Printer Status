#include <stdio.h>
int main() 
{
    int status;
    printf("Printer Status Menu:\n1. Ready\n2. Printing\n3. Out of Paper\n4. Error\nEnter status: ");
    scanf("%d", &status);
    switch(status) 
{
        case 1: printf("Printer is READY.\n"); break;
        case 2: printf("Printer is PRINTING.\n"); break;
        case 3: printf("Printer is OUT OF PAPER.\n"); break;
        case 4: printf("Printer ERROR.\n"); break;
        default: printf("Invalid status.\n");
    }
    return 0;
}
