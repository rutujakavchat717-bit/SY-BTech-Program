#include <stdio.h>
void main() 
{
    int duty_cycle, period;
    printf("Enter duty cycle (PWM 4): "); 
    scanf("%d", &duty_cycle);
    printf("Enter period: "); 
    scanf("%d", &period);
    printf("PWM 4: ");
    for(int i = 0; i < period; i++)
        printf(i < duty_cycle ? "1" : "0");
    printf("\n");
}
