#include <stdio.h>
float totalWater(float w[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += w[i];
    return total;
}
int main() 
{
    float w[5];
    printf("Enter water usage (in liters) for 5 days: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &w[i]);
    printf("Total Water Used = %.2f L\n", totalWater(w, 5));
    return 0;
}
