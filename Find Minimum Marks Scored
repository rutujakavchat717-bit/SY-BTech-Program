#include <stdio.h>
float minMarks(float m[], int n) 
{
    float min = m[0];
    for(int i = 1; i < n; i++)
        if(m[i] < min)
            min = m[i];
    return min;
}
int main() 
{
    float m[5];
    printf("Enter marks of 5 students: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &m[i]);
    printf("Minimum Marks = %.2f\n", minMarks(m, 5));
    return 0;
}
