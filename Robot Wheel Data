#include <stdio.h>

struct Wheel 
{
    int id;
    char type[30];
    float diameter;
    char status[20];
};

int main() 
{
    struct Wheel w;

    printf("Enter Wheel ID: ");
    scanf("%d", &w.id);

    printf("Enter Wheel Type: ");
    scanf("%s", w.type);

    printf("Enter Diameter (cm): ");
    scanf("%f", &w.diameter);

    printf("Enter Status: ");
    scanf("%s", w.status);

    printf("\n WHEEL INFORMATION \n");
    printf("ID: %d\n", w.id);
    printf("Type: %s\n", w.type);
    printf("Diameter: %.2f cm\n", w.diameter);
    printf("Status: %s\n", w.status);

    return 0;
}
