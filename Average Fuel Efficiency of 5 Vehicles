#include <stdio.h>
float avgMileage(float m[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += m[i];
    return total / n;
}
int main() 
{
    float m[5];
    printf("Enter mileage (km/l) of 5 vehicles: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &m[i]);
    printf("Average Mileage = %.2f km/l\n", avgMileage(m, 5));
    return 0;
}
