#include <stdio.h>
float totalMarks(float marks[], int n) 
{
    float sum = 0;
    for(int i = 0; i < n; i++)
        sum += marks[i];
    return sum;
}
int main() 
{
    float marks[5];
    printf("Enter total marks of 5 students: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &marks[i]);
    printf("Total of All Students = %.2f\n", totalMarks(marks, 5));
    return 0;
}
