#include <stdio.h>

void main() 
{
    float aqi[5] = {55.5, 72.3, 101.2, 130.8, 160.4};
    float *ptr;
    int i;

    ptr = aqi;

    printf("Air Quality Index Readings:\n");
    for(i=0; i<5; i++) 
    {
        printf("aqi[%d] = %.2f\n", i, aqi[i]);
    }

    printf("\nPointer Display:\n");
    for(i=0; i<5; i++) 
    {
        printf("AQI %d: %.2f\n", i+1, *(ptr+i));
    }
}
