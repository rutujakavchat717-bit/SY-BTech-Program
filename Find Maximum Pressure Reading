#include <stdio.h>
float maxPressure(float p[], int n) 
{
    float max = p[0];
    for(int i = 1; i < n; i++)
        if(p[i] > max)
            max = p[i];
    return max;
}
int main() 
{
    float p[5];
    printf("Enter 5 pressure readings (in Pa): ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &p[i]);
    printf("Maximum Pressure = %.2f Pa\n", maxPressure(p, 5));
    return 0;
}
