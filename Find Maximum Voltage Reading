#include <stdio.h>
float maxVoltage(float v[], int n) 
{
    float max = v[0];
    for(int i = 1; i < n; i++)
        if(v[i] > max)
            max = v[i];
    return max;
}
int main() 
{
    float v[5];
    printf("Enter 5 voltage readings: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &v[i]);
    printf("Maximum Voltage = %.2f V\n", maxVoltage(v, 5));
    return 0;
}
