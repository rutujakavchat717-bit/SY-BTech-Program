#include <stdio.h>
float maxLoad(float l[], int n) 
{
    float max = l[0];
    for(int i = 1; i < n; i++)
        if(l[i] > max)
            max = l[i];
    return max;
}
int main() 
{
    float l[5];
    printf("Enter load on 5 machines (in kg): ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &l[i]);
    printf("Maximum Load = %.2f kg\n", maxLoad(l, 5));
    return 0;
}
