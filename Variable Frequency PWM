#include <stdio.h>
#include <unistd.h>
void main()
{
printf("PWM HARD 7: Variable Frequency\n");
    for (int freq = 2; freq <= 10; freq += 2) 
{
        printf("Frequency = %d Hz -> ", freq);
        for (int i = 0; i < 10; i++)
            printf(i % freq == 0 ? "1" : "0");
        printf("\n");
}
}
