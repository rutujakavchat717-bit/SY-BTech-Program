#include <stdio.h>
#include <string.h>
int main() 
{
    char command[10];
    printf("Enter Command (WATER / STOP): ");
    scanf("%s", command);
    if (strcmp(command, "WATER") == 0)
        printf("Irrigation System Watering Plants\n");
    else if (strcmp(command, "STOP") == 0)
        printf("Irrigation Stopped\n");
    else
        printf("Invalid Command\n");
    return 0;
}
