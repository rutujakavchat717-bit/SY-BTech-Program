#include <stdio.h>
float totalSalary(float s[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += s[i];
    return total;
}
int main() 
{
    float s[5];
    printf("Enter salary of 5 employees: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &s[i]);
    printf("Total Salary = %.2f\n", totalSalary(s, 5));
    return 0;
}
