#include <stdio.h>
float avgResistance(float r[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += r[i];
    return total / n;
}
int main() 
{
    float r[5];
    printf("Enter resistance of 5 resistors (in ohms): ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &r[i]);
    printf("Average Resistance = %.2f ohms\n", avgResistance(r, 5));
    return 0;
}
