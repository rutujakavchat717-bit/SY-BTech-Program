#include <stdio.h>
float average(float marks[], int size) 
{
    float sum = 0;
    for(int i = 0; i < size; i++)
        sum += marks[i];
    return sum / size;
}
int main() 
{
    float marks[5];
    printf("Enter marks of 5 subjects: ");
    for(int i = 0; i < 5; i++)
    scanf("%f", &marks[i]);
    printf("Average Marks = %.2f\n", average(marks, 5));
    return 0;
}
