#include <stdio.h>
#include <string.h>
int main() 
{
    char action[10];
    printf("Enter Action (OPEN / CLOSE): ");
    scanf("%s", action);
    if (strcmp(action, "OPEN") == 0)
        printf("Windows Opening Automatically\n");
    else if (strcmp(action, "CLOSE") == 0)
        printf("Windows Closing Automatically\n");
    else
        printf("Invalid Action\n");
    return 0;
}
