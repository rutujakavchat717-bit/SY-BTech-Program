#include <stdio.h>
float highest(float marks[], int n) 
{
    float max = marks[0];
    for(int i = 1; i < n; i++)
        if(marks[i] > max)
            max = marks[i];
    return max;
}
int main() 
{
    float marks[5];
    printf("Enter marks of 5 students: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &marks[i]);
    printf("Highest Marks = %.2f\n", highest(marks, 5));
    return 0;
}
