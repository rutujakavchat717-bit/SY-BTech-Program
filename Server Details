#include <stdio.h>

struct Server 
{
    int id;
    char name[30];
    float load;
    char status[20];
};

int main() 
{
    struct Server s;

    printf("Enter Server ID: ");
    scanf("%d", &s.id);

    printf("Enter Server Name: ");
    scanf("%s", s.name);

    printf("Enter Load: ");
    scanf("%f", &s.load);

    printf("Enter Status: ");
    scanf("%s", s.status);

    printf("\n SERVER DETAILS \n");
    printf("ID: %d\n", s.id);
    printf("Name: %s\n", s.name);
    printf("Load: %.2f\n", s.load);
    printf("Status: %s\n", s.status);

    return 0;
}
