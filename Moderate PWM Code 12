#include <stdio.h>
void main() 
{
    int duty, period;
    printf("Enter duty cycle: "); scanf("%d", &duty);
    printf("Enter period: "); scanf("%d", &period);
    int array[period];
    for(int i = 0; i < period; i++)
        array[i] = (i < duty) ? 1 : 0;

    printf("PWM 12 (Stored in Array): ");
    for(int i = 0; i < period; i++)
        printf("%d", array[i]);
    printf("\n");
}
