#include <stdio.h>
#include <string.h>
int main() 
{
    char state[10];
    printf("Enter State (ON / OFF): ");
    scanf("%s", state);
    if (strcmp(state, "ON") == 0)
        printf("Humidifier Turned ON\n");
    else if (strcmp(state, "OFF") == 0)
        printf("Humidifier Turned OFF\n");
    else
        printf("Invalid State\n");
    return 0;
}
