#include <stdio.h>

struct DroneMotor 
{
    int id;
    char model[30];
    float rpm;
    char status[20];
};

int main() 
{
    struct DroneMotor dm;

    printf("Enter Motor ID: ");
    scanf("%d", &dm.id);

    printf("Enter Model: ");
    scanf("%s", dm.model);

    printf("Enter RPM: ");
    scanf("%f", &dm.rpm);

    printf("Enter Status: ");
    scanf("%s", dm.status);

    printf("\n DRONE MOTOR DETAILS \n");
    printf("ID: %d\n", dm.id);
    printf("Model: %s\n", dm.model);
    printf("RPM: %.2f\n", dm.rpm);
    printf("Status: %s\n", dm.status);

    return 0;
}

