#include <stdio.h>
float maxValue(float arr[], int n) 
{
    float max = arr[0];
    for(int i = 1; i < n; i++)
        if(arr[i] > max)
            max = arr[i];
    return max;
}
int main() 
{
    float nums[5];
    printf("Enter 5 numbers: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &nums[i]);
    printf("Maximum = %.2f\n", maxValue(nums, 5));
    return 0;
}
