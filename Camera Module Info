#include <stdio.h>

struct Camera 
{
    int id;
    char model[30];
    float resolution;
    char status[20];
};

int main() 
{
    struct Camera c;

    printf("Enter Camera ID: ");
    scanf("%d", &c.id);

    printf("Enter Model: ");
    scanf("%s", c.model);

    printf("Enter Resolution (MP): ");
    scanf("%f", &c.resolution);

    printf("Enter Status: ");
    scanf("%s", c.status);

    printf("\n CAMERA MODULE DETAILS \n");
    printf("ID: %d\n", c.id);
    printf("Model: %s\n", c.model);
    printf("Resolution: %.2f MP\n", c.resolution);
    printf("Status: %s\n", c.status);

    return 0;
}
