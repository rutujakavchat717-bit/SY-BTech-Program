#include <stdio.h>
float totalMarks(float marks[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += marks[i];
    return total;
}
int main() 
{
    float marks[5];
    printf("Enter marks of 5 subjects: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &marks[i]);
    printf("Total Marks = %.2f\n", totalMarks(marks, 5));
    return 0;
}
