#include <stdio.h>
float maxTemp(float t[], int n) 
{
    float max = t[0];
    for(int i = 1; i < n; i++)
        if(t[i] > max)
            max = t[i];
    return max;
}
int main() 
{
    float t[5];
    printf("Enter 5 temperature readings: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &t[i]);
    printf("Maximum Temperature = %.2fÂ°C\n", maxTemp(t, 5));
    return 0;
}
