#include <stdio.h>
#include <string.h>
int main() 
{
    char action[10];
    printf("Enter Action (CLEAN / RETURN): ");
    scanf("%s", action);
    if (strcmp(action, "CLEAN") == 0)
        printf("Robot Started Cleaning\n");
    else if (strcmp(action, "RETURN") == 0)
        printf("Robot Returning to Dock\n");
    else
        printf("Invalid Action\n");
    return 0;
}
