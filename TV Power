#include <stdio.h>
#include <string.h>
int main() 
{
    char command[10];
    printf("Enter Command (POWERON / POWEROFF): ");
    scanf("%s", command);
    if (strcmp(command, "POWERON") == 0)
        printf("TV is ON\n");
    else if (strcmp(command, "POWEROFF") == 0)
        printf("TV is OFF\n");
    else
        printf("Invalid Command\n");
    return 0;
}
