#include <stdio.h>
float avgPulse(float p[], int n) 
{
    float total = 0;
    for(int i = 0; i < n; i++)
        total += p[i];
    return total / n;
}
int main() 
{
    float p[5];
    printf("Enter 5 pulse rate readings: ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &p[i]);
    printf("Average Pulse Rate = %.2f bpm\n", avgPulse(p, 5));
    return 0;
}
