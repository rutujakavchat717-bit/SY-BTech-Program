#include <stdio.h>
float avgPower(float p[], int n) 
{
    float sum = 0;
    for(int i = 0; i < n; i++)
        sum += p[i];
    return sum / n;
}
int main() 
{
    float p[5];
    printf("Enter output power of 5 motors (in watts): ");
    for(int i = 0; i < 5; i++)
        scanf("%f", &p[i]);
    printf("Average Motor Power = %.2f W\n", avgPower(p, 5));
    return 0;
}
