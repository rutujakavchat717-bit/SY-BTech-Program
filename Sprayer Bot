#include <stdio.h>
#include <string.h>
int main() 
{
    char mode[10];
    printf("Enter Mode (SPRAY / PAUSE): ");
    scanf("%s", mode);
    if (strcmp(mode, "SPRAY") == 0)
        printf("Sprayer Activated\n");
    else if (strcmp(mode, "PAUSE") == 0)
        printf("Sprayer Paused\n");
    else
        printf("Invalid Mode\n");
    return 0;
}
